    "## 2. Funciones"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "id": "uaAz-C8rjbLM"
   },
   "outputs": [],
   "source": [
    "### Propagador por transformada de Fourier Rápida usando la función de transferencia ###\n",
    "#Simula la propagación de luz usando la función de transferencia\n",
    "\n",
    "#u1 : campo óptico de entrada\n",
    "#wl : longitud de onda\n",
    "#z : distancia de propagación\n",
    "#dx : muestreo espacial\n",
    "\n",
    "#retorna el campo óptico a una distancia z\n",
    "\n",
    "def propaTF(u1,wl,z,dx):\n",
    "  k = 2*np.pi/wl\n",
    "  tam = len(u1)\n",
    "  df = 1/(tam*dx)\n",
    "  f = np.arange(-tam*df/2,tam*df/2,df)\n",
    "  Fx , Fy = np.meshgrid(f,f)\n",
    "  H = np.exp(-1j*np.pi*wl*z*(Fx**2 + Fy**2))\n",
    "  U1 = np.fft.fftshift(np.fft.fft2(np.fft.fftshift(u1)))\n",
    "  return np.exp(1j*k*z)*np.fft.fftshift(np.fft.ifft2(np.fft.fftshift(U1*H)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "id": "Ehskx9_FPwvq"
   },
   "outputs": [],
   "source": [
    "###### Pupíla circular (tomada de Internet) #######\n",
    "#Creates a circular pupil inside a matrix\n",
    "\n",
    "#a,b: index of center of matrix (in real matrix position)\n",
    "#N: base array size\n",
    "#radius: pupil radius in pixels\n",
    "\n",
    "#array: final matrix\n",
    "#\"\"\"\n",
    "def cmask(a,b,radius,N):\n",
    "  y,x = np.ogrid[-a:N-a,-b:N-b]\n",
    "  mask = x*x + y*y <= radius*radius\n",
    "  array = np.zeros((N, N))\n",
    "  array[mask] = 1.\n",
    "\n",
    "  return array\n",
    "\n",
    "##### Marco o cuadro ######\n",
    "# Definición de función para construir marco de ceros sobre un recuadro o extraer un recuadro de un marco\n",
    "\n",
    "# cuadro: Matriz original que se quiere enmaracar o de la que se quiere extraer un recuadro\n",
    "# factor: tamaño relativo del marco o del recuadro.\n",
    "# tipo: define si se quiere crear un \"marco\" o extraer un \"recuadro\"\n",
    "\n",
    "def marco(cuadro,factor=1,tipo='marco'):\n",
    "  # cuadro: arreglo a enmarcar, factor: tamaño relativo del marco (debe ser mayor o igual a 1)\n",
    "  if(tipo == 'marco'):\n",
    "    N = len(cuadro)\n",
    "    if(cuadro.dtype == 'complex128'): \n",
    "      PE = np.zeros((factor*N,factor*N)) + 1j*np.zeros((factor*N,factor*N))\n",
    "    else:\n",
    "      PE = np.zeros((factor*N,factor*N))\n",
    "    PE[int(factor*N/2-N/2):int(factor*N/2+N/2),int(factor*N/2-N/2):int(factor*N/2+N/2)] = cuadro\n",
    "  elif(tipo == 'cuadro'):\n",
    "    N = len(cuadro)\n",
    "    PE = cuadro[int(N/2-factor*N/2):int(N/2+factor*N/2),int(N/2-factor*N/2):int(N/2+factor*N/2)]\n",
    "\n",
    "  return PE\n",
    "\n",
    "#### Translada recuadro ####\n",
    "# Para ubicar un recuadro en una posición no centrada del marco\n",
    "\n",
    "# cuadro: recuadro a tranladar en el marco\n",
    "# factor: tamaño relativo del marco\n",
    "# centrox,centroy: posición relativa del recuadro\n",
    "\n",
    "# Definición de función para transladar un recuadro dentro de un marco\n",
    "def trasla(cuadro,factor=2,centrox=1/2,centroy=1/2):\n",
    "  N = len(cuadro)\n",
    "  PE = np.zeros((factor*N,factor*N))\n",
    "  PE[int(factor*N*centroy-N/2):int(factor*N*centroy+N/2),int(factor*N*centrox-N/2):int(factor*N*centrox+N/2)] = cuadro\n",
    "  return PE"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "UkAdDgHva64z"
