    "## Solución analítica y comparación con el experimental (normalizado)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {
    "id": "c2dRidJtm0bX"
   },
   "outputs": [],
   "source": [
    "from scipy.special import j1\n",
    "#Definición de función\n",
    "def agujero(R,wl,f,dx,N):\n",
    "  x = np.arange(-N*dx/2,N*dx/2,dx)    # Creación del espacio coordenado\n",
    "  X, Y = np.meshgrid(x,x)   # espacio coordenado bidimensional\n",
    "  r = np.sqrt(X**2 + Y**2)\n",
    "  #Amp = np.nan_to_num(j1(1,2*np.pi*r/R)/r)\n",
    "  Amp = j1(2*np.pi*r*R/wl/f)/(np.pi*r*R/wl/f)\n",
    "  return Amp"
   ]
  },
  {
