    "plt.title('Fase')\n",
    "plt.imshow(np.angle(POb2),cmap='gray',extent=(-factor*incre*N*dx/2,factor*incre*N*dx/2-dx,-factor*incre*N*dx/2,factor*incre*N*dx/2-dx),origin='lower')\n",
    "plt.xlabel('mm')\n",
    "plt.ylabel('mm')\n",
    "plt.colorbar(label='rad',shrink=0.8)\n",
    "plt.subplot(133)\n",
    "plt.title('Irradiancia')\n",
    "plt.imshow(abs(POb2)**2,cmap='gray',extent=(-factor*incre*N*dx/2,factor*incre*N*dx/2-dx,-factor*incre*N*dx/2,factor*incre*N*dx/2-dx),origin='lower')\n",
    "plt.xlabel('mm')\n",
    "plt.ylabel('mm')\n",
    "plt.colorbar(shrink=0.8)\n",
